{
  "version": 2,
  "builds": [
    {
      "src": "pokemon-card-analyzer/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "pokemon-card-analyzer/server.js"
    },
    {
      "src": "/pokemon-card-analyzer/api/(.*)",
      "dest": "pokemon-card-analyzer/server.js"
    },
    {
      "src": "/pokemon-card-analyzer/(.*)",
      "dest": "pokemon-card-analyzer/server.js"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
