{
  "version": 2,
  "builds": [
    {
      "src": "pokemon-card-analyzer/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "pokemon-card-analyzer/server.js"
    },
    {
      "src": "/pokemon-card-analyzer/(.*)",
      "dest": "pokemon-card-analyzer/server.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|json))",
      "dest": "/$1"
    }
  ]
}
